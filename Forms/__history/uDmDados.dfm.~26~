  object cdsitens: TClientDataSet
    Aggregates = <>
    AutoCalcFields = False
    Params = <>
    OnCalcFields = cdsitensCalcFields
    Left = 288
    Top = 96
    object cdsitenscodigo: TStringField
      FieldName = 'codigo'
